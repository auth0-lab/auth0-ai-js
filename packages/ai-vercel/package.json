{
  "name": "@auth0/ai-vercel",
  "version": "0.0.0",
  "description": "",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"./**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "test": "echo 'missing tests'"
  },
  "imports": {
    "#interruptions": {
      "types": "./dist/interruptions/index.d.ts",
      "require": "./dist/interruptions/index.js",
      "import": "./dist/interruptions/index.mjs"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./interruptions": {
      "types": "./dist/interruptions/index.d.ts",
      "import": "./dist/interruptions/index.mjs",
      "require": "./dist/interruptions/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    }
  },
  "author": {
    "name": "Auth0 Inc.",
    "url": "https://auth0.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@auth0/ai": "*",
    "ai": "^4.1.46",
    "auth0": "^4.18.0"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "tsup": "^8.3.6",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "vitest": "^2.1.3"
  }
}
