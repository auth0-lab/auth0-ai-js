{
  "name": "@auth0/ai-vercel",
  "version": "0.0.0",
  "description": "",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:watch": "tsc --module ESNext --watch",
    "build:esm": "tsc --module ESNext --outDir ./dist/esm",
    "build:cjs": "tsc --module nodenext --moduleResolution nodenext --declaration false --declarationMap false --outDir ./dist/cjs",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "imports": {
    "#interrupts": {
      "types": "./dist/esm/interrupts/index.d.ts",
      "require": "./dist/cjs/interrupts/index.js",
      "import": "./dist/esm/interrupts/index.js"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./interrupts": {
      "types": "./dist/esm/interrupts/index.d.ts",
      "import": "./dist/esm/interrupts/index.js",
      "require": "./dist/cjs/interrupts/index.js"
    },
    "./react": {
      "types": "./dist/esm/react/index.d.ts",
      "import": "./dist/esm/react/index.js",
      "require": "./dist/cjs/react/index.js"
    }
  },
  "author": {
    "name": "Auth0 Inc.",
    "url": "https://auth0.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@auth0/ai": "*",
    "auth0": "^4.18.0",
    "z": "1.0.1"
  },
  "peerDependencies": {
    "ai": "^4.1.54",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.10",
    "eslint": "^9.22.0",
    "tsup": "^8.3.6",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.8"
  }
}
