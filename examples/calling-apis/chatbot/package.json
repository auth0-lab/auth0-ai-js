{
  "name": "@auth0/auth0-ai-js-examples-calling-apis-chatbot",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build:deps": "npm run build --workspace=@auth0/ai --workspace=@auth0/ai-genkit --workspace=@auth0/ai-langchain --workspace=@auth0/ai-llamaindex --workspace=@auth0/ai-vercel",
    "dev": "next dev",
    "start": "next start",
    "prebuild": "[ ! -f .env.local ] && cp .env.example .env.local || true",
    "build": "next build",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "all:dev": "run-p langgraph:dev dev",
    "all:start": "run-p langgraph:start start",
    "langgraph:start": "npx @langchain/langgraph-cli up",
    "langgraph:dev": "npx @langchain/langgraph-cli dev --port 54367",
    "langgraph:debug": "NODE_OPTIONS='--inspect=0.0.0.0:9229' npx @langchain/langgraph-cli dev --port 54367"
  },
