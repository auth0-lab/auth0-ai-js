VITE_AUTH0_AUDIENCE="contoso-api"

# Default LangGraph API URL for frontend
VITE_LANGGRAPH_API_URL="http://localhost:2024"
# Assistant/Graph ID (should match the graph ID in langgraph.json)
VITE_ASSISTANT_ID="memory_agent"